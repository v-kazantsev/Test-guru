<tr>
  <td><%= link_to test.title, admin_test_path(test) %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to t('.edit'), edit_admin_test_path(test) %>
  <td><%= link_to admin_test_path(test),#t('.delete'),
                  method: :delete,
                  data: { confirm: 'Are you sure?'} do %>
      <i class="fa fa-trash-o fa-2x" aria-hidden="true" style="color: red"></i>
      <% end %>
  </td>
</tr>
